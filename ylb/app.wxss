/**app.wxss**/
page{
  height: 100%;
}
.fl{
  float: left;
}
.fr{
  float: right;
}
.m-bbt{
  border-bottom:1px solid #E8E8E8;
}
.m-bg {
  background:#FAFAFA;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.plhStyle{
  color: #d1d1d1;
  font-size: 16px;
}
.m-cover{
  width: 100%;
  height: 100%;
  overflow:hidden;
}
.m-logo{
  width:70px;
  height:70px;
  position:absolute;
  left:50%;
  margin-left:-35px;
}



/* tabbar页面 */
.m-titleBox{
  font-size:16px;
  height:30px;
  line-height:30px;
  color:#666;
  margin:0 8px;
}
.m-dl{
  width:100%;
  height:65px;
  padding:12px 10px;
  background:#fff;

}
.m-dt{
  width:100px;
  height:65px;
  float:left;
  margin-right:10px;

}
.m-dd{

}
.m-dd>text{
  display:block;
  color:#999;
  font-size:14px;
  line-height:20px;
  height: 20px;
  overflow:hidden;
  margin-right: 20px;
}
.m-dd .tit{
  font-size:16px;
  color:#333;
  line-height:25px;
  height:25px;
}
swiper{
  margin-top: 1px;
}
.slide-image{
  width: 100%;
}
.m-line{
  margin: 0 14px;
  width: auto;
  height: 1px;
  background: #EFEFEF;
}
.m-boxText{
  background: #fff;
  margin: 20px;
  padding:20px;

}
.m-smallTip{
    font-size:12px;
    color:#999;
    line-height:20px;
}